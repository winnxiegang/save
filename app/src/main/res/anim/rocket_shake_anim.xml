<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@anim/side_bar_shake"
    android:repeatMode="reverse">
    <translate
        android:fromYDelta="0"
        android:toYDelta="5"
        android:duration="600"/>
    <translate
        android:fromXDelta="0"
        android:toXDelta="5"
        android:duration="600"/>
    <translate
        android:startOffset="600"
        android:fromYDelta="5"
        android:toYDelta="0"
        android:duration="600"/>
    <translate
        android:startOffset="600"
        android:fromXDelta="5"
        android:toXDelta="0"
        android:duration="600"/>
    <translate
        android:fromYDelta="0"
        android:toYDelta="5"
        android:duration="600"/>
    <translate
        android:fromXDelta="0"
        android:toXDelta="5"
        android:duration="600"/>
    <translate
        android:startOffset="600"
        android:fromYDelta="5"
        android:toYDelta="0"
        android:duration="600"/>
    <translate
        android:startOffset="600"
        android:fromXDelta="5"
        android:toXDelta="0"
        android:duration="600"/>
</set>