<?xml version="1.0" encoding="utf-8"?>

<set xmlns:android="https://schemas.android.com/apk/res/android">

    <translate

        android:duration="300"

        android:fromXDelta="0"

        android:fromYDelta="0"

        android:toXDelta="-10"

        android:toYDelta="-10" />

    <translate

        android:duration="300"

        android:fromXDelta="0"

        android:fromYDelta="0"

        android:startOffset="300"

        android:toXDelta="10"

        android:toYDelta="-10" />

    <translate

        android:duration="300"

        android:fromXDelta="0"

        android:fromYDelta="0"

        android:startOffset="600"

        android:toXDelta="-10"

        android:toYDelta="10" />

    <translate

        android:duration="300"

        android:fromXDelta="0"

        android:fromYDelta="0"

        android:startOffset="900"

        android:toXDelta="10"

        android:toYDelta="10" />


    <rotate
        android:fromDegrees="0"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="8"
        android:duration="300"

        >

    </rotate>
</set>