<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical"
    android:visibility="visible">
    <TextView
        android:background="@color/main_bg"
        android:text="管理员"
        android:id="@+id/identifyTitleTv"
        android:paddingTop="@dimen/dp5"
        android:paddingLeft="@dimen/dp10"
        android:paddingBottom="@dimen/dp5"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.android.banana.view.DragLinearLayout
        android:id="@+id/chatRoomMemberLL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white"
        android:orientation="horizontal"
        android:visibility="visible">


    <LinearLayout
        android:id="@+id/contentLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/selectIv"
            android:paddingLeft="10dp"
            android:paddingTop="22dp"
            android:paddingBottom="15dp"
            android:visibility="gone"
            android:src="@drawable/icon_contact_normal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <include layout="@layout/layout_chat_room_member_info"/>
    </LinearLayout>

    <LinearLayout
        android:background="@color/main_red"
        android:layout_width="60dp"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/slideDelete"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="删除"
            android:textColor="@color/white" />
    </LinearLayout>
    </com.android.banana.view.DragLinearLayout>
</LinearLayout>

