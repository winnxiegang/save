<?xml version="1.0" encoding="utf-8"?>
<com.android.banana.view.DragLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dragLinearLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="horizontal"
    android:visibility="visible">
    <LinearLayout
        android:id="@+id/contentLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <include layout="@layout/item_member_select"/>
    </LinearLayout>

    <LinearLayout
        android:background="@color/main_red"
        android:layout_width="100dp"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/slideDelete"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:textSize="@dimen/common_username_size"
            android:gravity="center"
            android:text="取消管理员"
            android:textColor="@color/white" />
    </LinearLayout>

</com.android.banana.view.DragLinearLayout>

